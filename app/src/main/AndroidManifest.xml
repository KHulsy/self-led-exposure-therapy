<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medikan.ptsd_treatment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayTimerActivity">
        </activity>
        <activity
            android:name=".TreatmentDescriptionPage"
            android:label="@string/title_activity_treatment_description_page">
        </activity>
        <activity android:name=".AvailableTreatmentsPage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".AvailableStepsPage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".AvailableTreatmentsPage" />
        </activity>
        <activity android:name=".TreatmentCompletionPage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".AvailableStepsPage" />
        </activity>
        <activity android:name=".MotivationPage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".TestInfoPage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".TestPage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TestInfoPage" />
        </activity>
        <activity android:name=".TestCompletedPage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>